{% if app.user %}
    <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('build/images/favIcons/apple-touch-icon-vault.png') }}">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('build/images/favIcons/favicon-32x32-vault.png') }}">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('build/images/favIcons/favicon-16x16-vault.png') }}">
    <link rel="manifest" href="{{ asset('build/images/favIcons/site.webmanifest-vault.json') }}">
    <link rel="mask-icon" href="{{ asset('build/images/favIcons/safari-pinned-tab-vault.svg') }}" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">
{% else %}
    <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('build/images/favIcons/apple-touch-icon.png') }}">
    <link rel="icon" type="image/png" sizes="32x32"
          href="{{ asset('build/images/favIcons/favicon-32x32.png') }}">
    <link rel="icon" type="image/png" sizes="16x16"
          href="{{ asset('build/images/favIcons/favicon-16x16.png') }}">
    <link rel="manifest" href="{{ asset('build/images/favIcons/site.webmanifest.json') }}">
    <link rel="mask-icon" href="{{ asset('build/images/favIcons/safari-pinned-tab.svg') }}" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">
{% endif %}