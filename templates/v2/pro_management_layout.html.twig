{% extends 'v2/pro_standard_layout.html.twig' %}
{% block metaTitle %}{{ 'membre.ajoutBeneficiaire.metaTitle'|trans }}{% endblock metaTitle %}

{% block actionBar %}
    <div class="col-8 col-sm-10 px-0 mt-3">
        <i class="fas fa-user-plus fa-lg me-1"></i>
        <span class="font-size-1-3 bold">{% block subTitle %}{% endblock %}</span>
    </div>
    <div class="col text-end pe-0 mt-4 mt-md-2">
        {% block homeButton %}
            <a href="{{ path('list_beneficiaries') }}"
               class="btn {{ getUserButtonColor() }} text-white">
                {{ 'back_home'|trans }}
            </a>
        {% endblock %}
    </div>
{% endblock %}
