{% extends app.user.beneficiaire
    ? 'v2/vault/list_container_layout.html.twig'
    : 'v2/standard_layout.html.twig' %}

{% block delimiter %}{% endblock %}
{% block metaTitle %}{{ 'my_relays_title'|trans }}{% endblock metaTitle %}

{% block content %}
    <div class="max-width-lg mt-4 m-auto">
        {% block list %}
            {{ userRelays is not empty
            ? include('v2/vault/relay/_list.html.twig', {userRelays})
            : include('v2/vault/components/_empty_list_content.html.twig', {
                message: 'center_has_no_info'|trans,
                img : asset('build/images/icons/relais_bleu.png'),
            }) }}
        {% endblock %}
    </div>
{% endblock %}
